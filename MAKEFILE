stamps=stamps\disc.st \
	stamps\credits.st \
	stamps\alku.st \
	stamps\beg.st \
	stamps\coman.st

all : $(stamps) 

stamps\disc.st:
	cd dis
	nmake disc.obj
	cd ..
	copy dis\disc.obj alku
	copy dis\disc.obj credits
	copy nul $@

stamps\alku.st:
	cd alku
	make -falku
	cd ..
	copy nul $@

stamps\beg.st:
	cd beg
	nmake
	cd ..
	copy nul $@

stamps\coman.st:
	cd coman
	nmake
	cd ..
	copy nul $@

stamps\credits.st:
	cd credits
	cd pics
	-cmd /c sfd
	cd ..
	-cmd /c unpack
	make -fcred
	cd ..
	copy nul $@
