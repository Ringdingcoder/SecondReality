stamps=stamps\disc.st \
	stamps\fcp.st \
	stamps\glenz.st \
	stamps\endscrl.st \
	stamps\endpic.st \
	stamps\end.st \
	stamps\dots.st \
	stamps\ddstars.st \
	stamps\credits.st \
	stamps\coman.st \
	stamps\beg.st \
	stamps\alku.st

all : $(stamps) 

stamps\disc.st:
	cd dis
	nmake disc.obj
	cd ..
	copy dis\disc.obj alku
	copy dis\disc.obj credits
	copy dis\disc.obj endscrl
	copy nul $@

stamps\alku.st:
	cd alku
	make -falku
	cd ..
	copy nul $@

stamps\beg.st:
	cd beg
	nmake
	cd ..
	copy nul $@

stamps\coman.st:
	cd coman
	nmake
	cd ..
	copy nul $@

stamps\credits.st:
	cd credits
	cd pics
	-cmd /c sfd
	cd ..
	-cmd /c unpack
	make -fcred
	cd ..
	copy nul $@

stamps\ddstars.st:
	cd ddstars
	nmake
	cd ..
	copy nul $@

stamps\dots.st:
	cd dots
	nmake
	cd ..
	copy nul $@

stamps\end.st:
	cd end
	nmake
	cd ..
	copy nul $@

stamps\endpic.st:
	cd endpic
	nmake
	cd ..
	copy nul $@

stamps\endscrl.st:
	cd endscrl
	make -fendscrl
	cd ..
	copy nul $@

stamps\fcp.st:
	cd fcp
	make -ffcp fcprot.exe
	cd ..
	copy nul $@

stamps\glenz.st:
	cd glenz
	nmake
	cd ..
	copy nul $@
